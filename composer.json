    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "zodiacmedia/drupal-libraries-installer": true
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "sh ./scripts/composer/post-install.sh"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/sites/brasilit.com.br/libraries/{$name}": ["type:drupal-library"],
            "web/sites/brasilit.com.br/modules/contrib/sg_core/{$name}": [
                "drupal/acquia_connector",
                "drupal/acquia_purge",
                "drupal/admin_toolbar",
                "drupal/adminimal_admin_toolbar",
                "drupal/captcha",
                "drupal/coder",
