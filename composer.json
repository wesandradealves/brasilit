                "drupal/config_split",
                "drupal/ctools",
                "drupal/devel",
                "drupal/google_analytics",
                "drupal/metatag",
                "drupal/pathauto",
                "drupal/purge",
                "drupal/redirect",
                "drupal/robotstxt",
                "drupal/search_api",
                "drupal/search_api_solr",
                "drupal/shield",
                "drupal/simple_sitemap",
                "drupal/token",
                "drupal/twig_xdebug",
                "drupal/webform"
            ],
            "web/themes/contrib/sg_core/{$name}": [
                "drupal/adminimal_admin_toolbar"
            ],
            "web/sites/brasilit.com.br/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/sites/brasilit.com.br/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "excludes": [
                ".htaccess"
            ]
        },
        "patches": {
          "drupal/core": {
            "Link to Entity get invalid language link in views": "https://www.drupal.org/files/issues/2862511-24.patch",
            "Dots in query parameter names in external menu links converted to underscores" : "https://www.drupal.org/files/issues/2018-07-19/dots-in-query-parameter-names-in-externa-urls-converted-to-underscores-2984272-5.patch",
            "Entity reference field View output is not used for selected entity display": "https://www.drupal.org/files/issues/2018-11-27/entityreference_autocomplete_views_edit-2796341-18.patch",
            "Fatal error update 8702 createHandlerInstance invoked with NULL class object.": "https://www.drupal.org/files/issues/2019-05-13/3053865-7.patch",
            "Failed while updating from 8.6.15 to 8.7.x: Update aborted by: menu_link_content_post_update_make_menu_link_content_revisionable": "https://www.drupal.org/files/issues/2019-05-28/3052905-core-update-8-7.patch",
            "Edit links on the translations overview page do not point to the edit form of translation!": "https://www.drupal.org/files/issues/2019-05-26/2451693-48-content_translation-links.patch",
            "Vocabulary terms list not working with pager.": "https://www.drupal.org/files/issues/2019-06-21/vocabulary_terms_list_not_working_with_pager-242324-119_0.patch",
            "Uncaught TypeError: Cannot read property 'replace' of undefined": "https://www.drupal.org/files/issues/uncaught_type_error_cannot_read_property_replace_of_undefined-2922677-5.patch",
            "Unsupported operand types in Drupal\\views\\Entity\\View->mergeDefaultDisplaysOptions()": "https://www.drupal.org/files/issues/2019-10-22/unsupported-types-2952261-11.patch",
            "2925890 - Invalid config structures can result in exceptions when saving a config entity": "https://www.drupal.org/files/issues/2020-08-13/2925890-18.patch"
          },
          "drupal/bootstrap_layouts": {
            "Is there a way to add custom classes on region or layout? [3003006-2]":"https://www.drupal.org/files/issues/2018-10-24/is_there_way_add_custom_class_layout-3003006-2.patch"
          },
          "drupal/classy_paragraphs": {
            "2830403 - Allow to filter classy styles": "https://www.drupal.org/files/issues/choose_and_order-2830403-11.patch"
          },
          "drupal/facets": {
            "Facet is not displayed when the dependent processor and renders the items using hierarchy is active [3008061-5]": "https://www.drupal.org/files/issues/2018-10-20/check_children-3008061-5.patch"
          },
          "drupal/page_manager": {
            "2665328 - Variants take entity label not the variant title": "https://www.drupal.org/files/issues/2020-09-15/page_manager-2665328-111.patch",
            "2810329 - Page Manager paths should support optional arguments as well": "https://www.drupal.org/files/issues/2020-06-04/page_manager-allow_optional_parameters-2810329-16.patch"
          },
          "drupal/panels": {
            "Changes to Layout not being saved.": "https://www.drupal.org/files/issues/2018-08-28/panels-fix_updating_layout-2824632-16-D8.patch"
          }
        },
        "patches-ignore": {
            "acquia/lightning": {
                "drupal/core": {
                    "[subprofile support] 1356276 - Allow profiles to define a base/parent profile and load them in the correct order / 2914389 - Allow profiles to exclude dependencies of their parent": "https://www.drupal.org/files/issues/2021-01-14/3143958-11-subprofile-support-8.9.x.patch",
                    "REMOVE: Allow installation profile to be changed from Lightning during config sync": "https://www.drupal.org/files/issues/2021-01-08/3143958-10-change-profile-config-sync-9.1.x.patch"
                }
            },
            "drupal/lightning_layout": {
                "drupal/panels": {
                    "2878684 - Use String.match to correlate regions when switching Layouts in Panels IPE": "https://www.drupal.org/files/issues/panels-ipe-2878684-3.patch"
                }
            }
        },
        "merge-plugin": {
            "include": [
                "web/sites/brasilit.com.br/modules/contrib/sg_core/webform/composer.libraries.json",
                "composer.libraries.json"
            ]
        }
    },
